{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "api-gateway",
  "main": "src/index.ts",
  "compatibility_date": "2025-04-18",
  "observability": {
    "enabled": true
  },
  "routes": [
    {
      "pattern": "api.402systems.com",
      "custom_domain": true
    }
  ],
  "services": [
    {
      "binding": "DB_DEMO_API",
      "service": "db-demo-api"
    }
  ]
}
