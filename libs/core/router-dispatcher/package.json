{
  "name": "@402systems/lib-router-dispatcher",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "build": "pnpm run codegen:wrangler",
    "codegen:wrangler": "wrangler types",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\" --ignore-path ../../../.prettierignore",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\" --ignore-path ../.../../.prettierignore"
  },
  "devDependencies": {
    "@402systems/lib-core-eslint": "workspace:*",
    "@402systems/lib-core-tsconfig": "workspace:*",
    "typescript": "latest",
    "wrangler": "^4.54.0"
  }
}
